{
  "version": 1,
  "projects": {
    "control-tower": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/control-tower",
      "sourceRoot": "apps/control-tower/src",
      "prefix": "spmka",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/control-tower",
            "index": "apps/control-tower/src/index.html",
            "main": "apps/control-tower/src/main.ts",
            "polyfills": "apps/control-tower/src/polyfills.ts",
            "tsConfig": "apps/control-tower/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/control-tower/src/favicon.ico",
              "apps/control-tower/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/control-tower/src/styles.scss",
              "libs/shared/styles/shared-styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/control-tower/src/environments/environment.ts",
                  "with": "apps/control-tower/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "control-tower:build",
            "port": 4001
          },
          "configurations": {
            "production": {
              "browserTarget": "control-tower:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "control-tower:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/control-tower/tsconfig.app.json",
              "apps/control-tower/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/control-tower/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/control-tower/jest.config.js",
            "tsConfig": "apps/control-tower/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/control-tower/src/test-setup.ts"
          }
        }
      }
    },
    "control-tower-e2e": {
